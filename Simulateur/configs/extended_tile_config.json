{
    "global_params": {
        "debug": false,
        "n_rows" : 15,
        "n_cols" : 15,
        "tile_size": 1,
        "track_width": 0.5,
        "tile_resolution": 500,
        "dilation_kernel_size": 12,
        "dilation_iter" : 3
    },
    "tiles": [
        {
            "name": "straight",
            "equations": {
                "top": "(tile_size * (1 + track_width) / 2)",
                "bottom": "(tile_size * (1 - track_width) / 2)"
            },
            "domains": {
                "top": [
                    "0",
                    "tile_size"
                ],
                "bottom": [
                    "0",
                    "tile_size"
                ]
            },
            "sockets": {
                "top": null,
                "bottom": null,
                "left": "mid",
                "right": "mid"
            }
        },
        {
            "name": "curved",
            "equations": {
                "top": "sqrt(((tile_size + track_width) / 2)**2 - (x - tile_size)**2)",
                "bottom": "sqrt(((tile_size - track_width) / 2)**2 - (x - tile_size)**2)"
            },
            "domains": {
                "top": [
                    "(tile_size - track_width)/2",
                    "tile_size"
                ],
                "bottom": [
                    "(tile_size + track_width)/2",
                    "tile_size"
                ]
            },
            "sockets": {
                "top": null,
                "right": "mid",
                "bottom": "mid",
                "left": null
            }
        },
        {
            "name": "divider_down",
            "equations": {
                "top": "(tile_size+track_width)/2",
                "left_bottom": "sqrt( ((tile_size-track_width)/2)**2 - x**2)",
                "right_top": "(tile_size-track_width)/2",
                "right_bottom": "sqrt( ((tile_size+track_width)/2)**2 - x**2)"
            },
            "domains": {
                "top": [
                    "0",
                    "tile_size"
                ],
                "left_bottom": [
                    "0",
                    "(tile_size - track_width)/2"
                ],
                "right_top": [
                    "sqrt(track_width * tile_size)",
                    "tile_size"
                ],
                "right_bottom": [
                    "sqrt(track_width * tile_size)",
                    "(tile_size + track_width)/2"
                ]
            },
            "sockets": {
                "top": null,
                "bottom": "mid",
                "left": "mid",
                "right": "mid"
            }
        },
        {
            "name": "divider_up",
            "equations": {
                "bottom": "(tile_size-track_width)/2",
                "left_top": "tile_size - sqrt( ((tile_size-track_width)/2)**2 - x**2)",
                "right_bottom": "(tile_size+track_width)/2",
                "right_top": "tile_size - sqrt( ((tile_size+track_width)/2)**2 - x**2)"
            },
            "domains": {
                "bottom": [
                    "0",
                    "tile_size"
                ],
                "left_top": [
                    "0",
                    "(tile_size - track_width)/2"
                ],
                "right_bottom": [
                    "sqrt(track_width * tile_size)",
                    "tile_size"
                ],
                "right_top": [
                    "sqrt(track_width * tile_size)",
                    "(tile_size + track_width)/2"
                ]
            },
            "sockets": {
                "top": "mid",
                "bottom": null,
                "left": "mid",
                "right": "mid"
            }
        },
        {
            "name": "empty",
            "equations": {
                "top": "0",
                "bottom": "0",
                "left": "0",
                "right": "0"
            },
            "domains": {
                "top": [
                    "0",
                    "0"
                ],
                "bottom": [
                    "0",
                    "0"
                ],
                "left": [
                    "0",
                    "0"
                ],
                "right": [
                    "0",
                    "0"
                ]
            },
            "sockets": {
                "top": null,
                "bottom": null,
                "left": null,
                "right": null
            }
        }
    ]
}